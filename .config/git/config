[alias]
  ad = add
  ca = commit -am
  cm = commit -m
  cf = commit -v --fixup=HEAD
  cn = commit --amend
  s = switch
  sc = switch -c
  ri = !sh -c 'git rebase -i HEAD~$0'
  rc = rebase --continue
  rh = reset HEAD^
  rs = !sh -c 'git reset HEAD~$0'
  pl = pull
  pr = pull --rebase
  ps = push
  pf = push --force-with-lease
  st = status
  dsf = "!f() { [ -z \"$GIT_PREFIX\" ] || cd \"$GIT_PREFIX\" && git diff --color \"$@\" | diff-so-fancy  | less --tabs=4 -RFX; }; f"
  l = log
  lp = log -p
  ll = log --graph --decorate --pretty=oneline --all --abbrev-commit
  ba = branch -a
  bd = branch -d
  bv = branch -v
  bdd = branch -D
  bm = branch -M
  su = stash -u
  sl = stash list
  sp = stash pop
  re = restore
  res = restore --staged
  cp= cherry-pick
  rv = remote -v
  
[include]
	path = ~/.gitconfig_shared
[core]
	commentchar = ~
[user]
	name = maimux2x
	email = m0smile10often@gmail.com
[ghq]
	root = ~/Documents/Source
[push]
	default = tracking
